@using MvcCv.Models.Entity
@model List<TblYeteneklerim>
<div class="resume-section-content">
    <h2 class="mb-5">YETENEKLERİM</h2>
    <div class="subheading mb-3">Programlama Dilleri & Araçlar</div>
    <ul class="list-inline dev-icons">
        <li class="list-inline-item"><i class="fab fa-html5"></i></li>
        <li class="list-inline-item"><i class="fab fa-css3-alt"></i></li>
        <li class="list-inline-item"><i class="fab fa-microsoft"></i></li> <!-- ASP.NET -->
        <li class="list-inline-item"><i class="fas fa-database"></i></li> <!-- MSSQL -->
        <li class="list-inline-item"><i class="fab fa-java"></i></li> <!-- Java -->
        <li class="list-inline-item"><i class="fas fa-network-wired"></i></li> <!-- Entity Framework -->
        <li class="list-inline-item"><i class="fas fa-code"></i></li> <!-- Code First -->
        <li class="list-inline-item"><i class="fab fa-git-alt"></i></li> <!-- Git -->
        <li class="list-inline-item"><i class="fab fa-github"></i></li> <!-- GitHub -->



    </ul>
    <div class="subheading mb-3">Becerilerim</div>
    <ul class="fa-ul mb-0">
        @foreach (var x in Model)
        {
        <li>
            <span class="fa-li"><i class="fas fa-check"></i></span>
           @x.Yetenek
        </li>
        }
    </ul>
</div>

